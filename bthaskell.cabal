  build-depends:
    base >=4.6 && <4.7,
    bencoding,
    bytestring,
    haskell-src,
    HTTP,
    cryptohash,
    http-types,
    split,
    async,
    network,
    binary,
    monads-tf,
    containers,
    stm,
    time,
    mmap      >=0.5 && <0.6,
    filepath  >=1.3 && <1.4,
    directory >=1.2 && <1.3

  -- hs-source-dirs:
  default-language:    Haskell2010
  ghc-options: -threaded
  -- -Wall -fno-warn-unused-do-bind
